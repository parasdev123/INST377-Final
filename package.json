{
  "name": "inst377-final",
  "version": "1.0.0",
  "description": "A webserver to pull PG county data and push it to a the front end",
  "engines": {
    "node": "13.7.0",
    "npm": "6.13.6"
  },
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run server"
  },
  "author": "Eltayeb Abdelrahman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parasdev123/INST377-Final"
  },
  "license": "CC-BY-SA-4.0",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "sqlite": "^4.0.7",
    "sqlite3": "^4.1.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "nodemon": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2"
  }
}
